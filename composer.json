{
    "name": "bgside/tourism-travel-crm",
    "type": "project",
    "description": "Enterprise Tourism & Travel CRM System - Comprehensive travel industry customer relationship management with multilingual support, booking management, and travel-specific workflows.",
    "keywords": [
        "tourism",
        "travel",
        "crm",
        "booking",
        "laravel",
        "travel-agency",
        "tour-operator",
        "multilingual",
        "arabic",
        "english",
        "xampp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Emad SALEH",
            "email": "contact@example.com",
            "homepage": "https://bgside.github.io/portfolio-website",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^3.8",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-translatable": "^6.8",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/laravel-backup": "^8.8",
        "spatie/laravel-medialibrary": "^11.9",
        "barryvdh/laravel-dompdf": "^3.0",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "^7.2",
        "laravel/cashier": "^15.4",
        "stripe/stripe-php": "^15.8",
        "paypal/rest-api-sdk-php": "^1.14",
        "predis/predis": "^2.2",
        "laravel/scout": "^10.11",
        "league/flysystem-aws-s3-v3": "^3.28",
        "laravel/socialite": "^5.15",
        "nesbot/carbon": "^3.8",
        "ramsey/uuid": "^4.7",
        "fakerphp/faker": "^1.23",
        "laravel/telescope": "^5.2",
        "laravel/horizon": "^5.29",
        "laravel/pulse": "^1.3",
        "filament/filament": "^3.2",
        "league/commonmark": "^2.5",
        "symfony/http-client": "^7.1",
        "symfony/mailgun-mailer": "^7.1",
        "livewire/livewire": "^3.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.17",
        "laravel/sail": "^1.31",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.4",
        "phpunit/phpunit": "^11.3",
        "spatie/laravel-ignition": "^2.8",
        "laravel/breeze": "^2.2",
        "pestphp/pest": "^3.2",
        "pestphp/pest-plugin-laravel": "^3.0",
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.1",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\Modules\\TravelPackages\\": "app/Modules/TravelPackages/",
            "App\\Modules\\Bookings\\": "app/Modules/Bookings/",
            "App\\Modules\\Customers\\": "app/Modules/Customers/",
            "App\\Modules\\Destinations\\": "app/Modules/Destinations/",
            "App\\Modules\\Payments\\": "app/Modules/Payments/",
            "App\\Modules\\Analytics\\": "app/Modules/Analytics/",
            "App\\Modules\\Communications\\": "app/Modules/Communications/",
            "App\\Modules\\Multilingual\\": "app/Modules/Multilingual/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "fresh": [
            "@php artisan migrate:fresh",
            "@php artisan db:seed --class=TourismDemoSeeder"
        ],
        "setup": [
            "composer install",
            "npm install",
            "@php artisan key:generate",
            "@php artisan storage:link",
            "@php artisan migrate:fresh --seed"
        ],
        "test": [
            "@php artisan test"
        ],
        "test-coverage": [
            "@php artisan test --coverage"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "cs-fix": [
            "./vendor/bin/php-cs-fixer fix"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models",
            "@php artisan ide-helper:meta"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/bgside"
        }
    ],
    "support": {
        "issues": "https://github.com/bgside/tourism-travel-crm/issues",
        "source": "https://github.com/bgside/tourism-travel-crm",
        "docs": "https://github.com/bgside/tourism-travel-crm/wiki"
    }
}