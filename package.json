{
    "name": "tourism-travel-crm",
    "version": "1.0.0",
    "description": "Enterprise Tourism & Travel CRM System - Frontend dependencies and build tools",
    "keywords": [
        "tourism",
        "travel",
        "crm",
        "booking",
        "vue",
        "laravel",
        "multilingual"
    ],
    "author": {
        "name": "Ali Emad SALEH",
        "email": "contact@example.com",
