    "license": "MIT",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "watch": "vite build --watch",
        "hot": "vite --host",
        "prod": "npm run build",
        "analyze": "vite-bundle-analyzer",
        "lint": "eslint resources/js --ext .js,.vue --fix",
        "lint:check": "eslint resources/js --ext .js,.vue",
        "format": "prettier --write resources/js/**/*.{js,vue,ts}",
        "format:check": "prettier --check resources/js/**/*.{js,vue,ts}",
        "type-check": "vue-tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage"
    },
    "devDependencies": {
        "@inertiajs/vue3": "^1.2.0",
